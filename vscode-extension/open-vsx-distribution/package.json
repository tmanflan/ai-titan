{
  "name": "ai-titan-deepseek-agent",
  "displayName": "AI Titan DeepSeek Agent",
  "description": "Powerful AI coding assistant powered by DeepSeek Coder for code generation, debugging, and optimization",
  "version": "1.0.0",
  "publisher": "ai-titan",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Snippets"
  ],
  "keywords": [
    "ai",
    "deepseek",
    "code generation",
    "assistant",
    "productivity"
  ],
  "activationEvents": [
    "onCommand:ai-titan.generateCode",
    "onCommand:ai-titan.optimizeCode",
    "onCommand:ai-titan.explainCode",
    "onCommand:ai-titan.debugCode"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-titan.generateCode",
        "title": "Generate Code",
        "category": "AI Titan"
      },
      {
        "command": "ai-titan.generateCodeDetailed",
        "title": "üìù Generate Code (Detailed)",
        "category": "AI Titan"
      },
      {
        "command": "ai-titan.optimizeCode",
        "title": "Optimize Selected Code",
        "category": "AI Titan"
      },
      {
        "command": "ai-titan.explainCode",
        "title": "Explain Selected Code",
        "category": "AI Titan"
      },
      {
        "command": "ai-titan.debugCode",
        "title": "Debug Code",
        "category": "AI Titan"
      },
      {
        "command": "ai-titan.askQuestion",
        "title": "üí¨ Ask AI Titan a Question",
        "category": "AI Titan"
      },
      {
        "command": "ai-titan.openSettings",
        "title": "Open Settings",
        "category": "AI Titan"
      },
      {
        "command": "ai-titan.viewPackages",
        "title": "View Warrior Packages",
        "category": "AI Titan"
      },
      {
        "command": "ai-titan.subscriptionStatus",
        "title": "Subscription Status",
        "category": "AI Titan"
      },
      {
        "command": "ai-titan.upgrade",
        "title": "Upgrade Package",
        "category": "AI Titan"
      },
      {
        "command": "ai-titan.enterOwnerCode",
        "title": "üîê Enter Owner Code",
        "category": "AI Titan"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "ai-titan.optimizeCode",
          "when": "editorHasSelection",
          "group": "ai-titan"
        },
        {
          "command": "ai-titan.explainCode",
          "when": "editorHasSelection",
          "group": "ai-titan"
        }
      ],
      "commandPalette": [
        {
          "command": "ai-titan.generateCode"
        },
        {
          "command": "ai-titan.generateCodeDetailed"
        },
        {
          "command": "ai-titan.optimizeCode",
          "when": "editorHasSelection"
        },
        {
          "command": "ai-titan.explainCode",
          "when": "editorHasSelection"
        },
        {
          "command": "ai-titan.debugCode"
        },
        {
          "command": "ai-titan.askQuestion"
        },
        {
          "command": "ai-titan.openSettings"
        },
        {
          "command": "ai-titan.viewPackages"
        },
        {
          "command": "ai-titan.subscriptionStatus"
        },
        {
          "command": "ai-titan.upgrade"
        },
        {
          "command": "ai-titan.enterOwnerCode"
        }
      ]
    },
    "configuration": {
      "title": "AI Titan DeepSeek Agent",
      "properties": {
        "ai-titan.apiKey": {
          "type": "string",
          "default": "",
          "description": "DeepSeek API Key",
          "scope": "application"
        },
        "ai-titan.model": {
          "type": "string",
          "default": "deepseek-coder",
          "description": "DeepSeek model to use",
          "enum": [
            "deepseek-coder",
            "deepseek-chat"
          ]
        },
        "ai-titan.maxTokens": {
          "type": "number",
          "default": 2048,
          "description": "Maximum tokens for response"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.0",
    "@vscode/vsce": "^2.15.0"
  },
  "dependencies": {
    "openai": "^4.0.0",
    "axios": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/ai-titan-deepseek-agent.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/ai-titan-deepseek-agent/issues"
  },
  "homepage": "https://github.com/your-username/ai-titan-deepseek-agent#readme",
  "license": "PROPRIETARY",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "pricing": "Trial",
  "author": "",
  "type": "commonjs"
}
